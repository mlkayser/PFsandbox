<aura:component implements="forceCommunity:availableForAllPageTypes" access="global">

    <aura:handler name="init" value="{!this}" action="{!c.getQuery}"/>
	<aura:attribute name="query" type="String" />
	<center>
        <iframe id="myiframe" src="{!'https://www.tfaforms.com/4711061' + v.query}" allowfullscreen="yes" height="800" frameborder="0" scrolling="yes"/>
    </center>
    
</aura:component>